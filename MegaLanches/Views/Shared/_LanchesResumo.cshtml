@model Lanche

<div class="col-sm-4 col-lg-4 col-md-4">
        <div class="thumbnail">
            <img src="@Model.ImagemUrl" class="img-rounded" alt="Foto do Lanche" >
        </div>
        <div class="caption">
            <h3 class="pull-right">@Model.Preco.ToString("c")</h3>
            <h3>@Model.Nome</h3>
            <p>@Model.DescricaoCurta</p>
        </div>
</div>